{
  "expo": {
    "name": "Pocket Weather",
    "slug": "pocket-weather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#241B3A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.testtrial.pocketweather"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#241B3A"
      }
    },
    "web": {
      "favicon": "./assets/app-icon.png"
    }
  }
}
